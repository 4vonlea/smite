RewriteEngine on
RewriteCond $1 !^(index\.php|resources|robots\.txt)
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^(.*)$ index.php/$1 [L,QSA]
RedirectMatch 404 /\.git
RedirectMatch 404 ^(.*/)?\.git+
<Files .env>
    Order allow,deny
    Deny from all
</Files>
<Directorymatch "^/.*/\.git+/">
    Order allow,deny
    Deny from all
</Directorymatch>
