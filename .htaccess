RewriteEngine on
RewriteCond $1 !^(index\.php|resources|robots\.txt)
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^(.*)$ index.php/$1 [L,QSA]
RedirectMatch 404 /\.git
RedirectMatch 404 ^(.*/)?\.git+
RedirectMatch 403 ^/\.git/?$
RewriteRule ^.*\.git.* - [R=404]
<Files .env>
    Order allow,deny
    Deny from all
</Files>
<DirectoryMatch "^/.*/\.git/">
    Require all denied
</DirectoryMatch>

<Files ~ "^\.git">
   Order allow,deny
   Deny from all 
   #Require all denied
</Files>
